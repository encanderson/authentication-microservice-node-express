generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRESQL_URI")
}

model User {
  id         String   @id @default(uuid())
  user_id    String   @unique
  active     Boolean
  app        String
  code       Int?
  created_at DateTime
  email      String   @unique
  password   String
  updated_at DateTime
  consents   Json?

  @@map("users")
}
